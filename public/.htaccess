RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteRule (.*) https://www.%{HTTP_HOST}/$2 [R=301,L]


<IfModule mod_rewrite.c>

RewriteEngine On
RewriteBase /
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-l
RewriteRule . /index.html [L]


</IfModule>